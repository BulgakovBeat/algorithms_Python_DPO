    "import math\n",
    "number = int(input())\n",
    "print()\n",
    "\n",
    "def isPrime(n):\n",
    "    s = round(math.sqrt(n)) + 1\n",
    "    for i in range(2, s, 1):\n",
    "        if n % i == 0:\n",
    "            return 'NO'\n",
    "    return 'YES' \n",
    "\n",
    "\n",
    "print(isPrime(number))"
   ]
