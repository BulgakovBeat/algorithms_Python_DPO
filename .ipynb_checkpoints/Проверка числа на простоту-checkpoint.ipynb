   "source": [
    "import math\n",
    "number = int(input())\n",
    "\n",
    "\n",
    "def isPrime(n):\n",
    "    s = round(math.sqrt(n))\n",
    "    print(s)\n",
    "    for i in range(2, s):\n",
    "        print(i)\n",
    "        if n % i == 0:\n",
    "            return 'NO'\n",
    "    return 'YES' \n",
    "\n",
    "\n",
    "isPrime(number)"
   ]
